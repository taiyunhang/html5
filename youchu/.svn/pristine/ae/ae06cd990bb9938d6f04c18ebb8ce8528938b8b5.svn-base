@charset "utf-8";
/* CSS Document */
html{font-size:62.5%;}
body{font-family:"微软雅黑";font-size:1.4rem; color: #000000; background:#e6e6e6;}
*{margin:0;padding:0;box-sizing:border-box;}
p{margin:0 0 5px;}
a:link{text-decoration:none;}
a:hover{text-decoration:none;}
ol, ul {list-style:none;margin-bottom: 0px;}
h4{font-size: 1.6rem;}
h5{font-size: 1.5rem;}
a{color:#fff;}
/*清除浮动 代码*/
.clearfix:after {
		visibility:hidden;
		display:block;
		font-size:0;
		content:" ";
		clear:both;
		height:0;
}
* html .clearfix             { zoom: 1; } /* IE6 */
*:first-child+html .clearfix { zoom: 1; } /* IE7 */
/* 主体 */
.dis{ display: block;}
.undis{ display:none;}
.top{ position:fixed;width:100%; z-index:1; text-align:center; font-size:1.8rem; line-height:30px;letter-spacing:2px;padding:0.8rem 0; background:#028c6c;}
/*活动页面*/
.apart a{display: block;}
.actcontent{background: url(../images/hdym_bg2.jpg); background-size: 100% 100%;letter-spacing:1px;color: #8E8E8E; padding: 90px 0px 20px;}
.acttext{line-height: 25px;padding: 0px 26px 35px;}
.apart{text-align:center;padding:10px 0px;width:60%;margin:0px auto 60px; cursor: pointer; background-color: #cd0920
;border-radius:2.0rem;color: #FFFFFF;font-size:15px;font-weight:600;}
.apart:hover{background-color:#b8081d;}
/*登录界面*/
.login{color: #FFFFFF;}
.back{ position:absolute; line-height:42px; padding:0 1.0rem; top:0;left:0;}
.mt{margin-top:46px;}
.list{margin:0 1.0rem;}
.prtin{position:relative;background-color:#FFFFFF; border:1px solid #cfcfcf; border-radius:0.4rem;}
.card{width: 100%; background-color:#FFFFFF; border:1px solid #cfcfcf; padding:1rem 0rem 1rem 1rem;border-radius:0.4rem;}
.validate{border:none;width:70%;padding:1rem;}
.yanzheng{ position:absolute;right:1rem;border:0;background:#fff;color:#00744b;padding:1rem 0;}
.fwjs{position:relative; border:1px solid #00744b; border-radius:0.4rem; margin:2.0rem 1.0rem;padding:15px 10px 10px;}
.serv{position:absolute;top:-13px;width:74px;}
.procar{ margin-bottom:10px;}
.procar p{color:#8e8e8e;}
.protitile{}
.submit{text-align:center;padding:10px 0px;margin:0 10px; cursor: pointer; background-color: #00744b;border-radius:0.4rem;}
.submit:hover{background-color: #006843;}
.submit span{font-size:1.6rem;color:#FFF;}
.subtn{margin-bottom:30px;margin-top: 22px;}
/*立即抢码*/
.qiangma{position: absolute;bottom: 16%;left:30%;width:40%;}
/*洗车服务*/
.wash{margin: 1rem;background-color: #FFFFFF; border:1px solid #cfcfcf; padding:1.2rem;border-radius:0.4rem;}
.wash p{color:#8e8e8e;margin-top: 1.5rem;}
.btn_code{margin-top: 1.5rem;}
.strcode{margin-top: 2rem;}
#strcode{color: #00744B;}
.submit img{margin-top: -6px;margin-right: 8px; }
.fadetwo1{ background:url(../images/dd_code_1.png); background-size:100% 100%;width:234px;height:234px;position:fixed;top:50%;left:50%;margin-left:-117px;margin-top:-117px;}
.fadetwo2{margin:44px;}
/*选择城市*/
.ct{ text-align:center; line-height:30px;padding-right:5px;padding-bottom:8px;padding-left:0px;}
.az{background:#FFF;font-size:1.2rem;color:#05377D;height:90%; position:fixed;top:46px;right:0;display:none;z-index:99;width:45px;line-height:28px;text-align:center;overflow:auto;padding-bottom:30px; }
.tel{font-size:1.6rem;background:#FFF;line-height:45px;}
.city-title{font-size:1.4rem;line-height:35px;color:#8E8E8E;background:#e6e6e6;margin-bottom:0.6rem;}
.citytel{font-size:1.6rem; color:#404040;background:#FFF;line-height:45px;}
.line{border-bottom:1px solid #F3F3F6}
/*消费记录*/
.recordlist{margin:12px 10px 0px;}
.dlist{ background:url(../images/xfjl_bg1.png); background-size:100% 100%;padding:10px 9px;}
.dlist p{font-size:1.6rem;padding-bottom:10px; border-bottom: 1px solid #B2B2B2; }
.td1{color:#B2B2B2;padding:6px 0px; vertical-align:top;width:90px;}
.td2{padding:0px 10px;color:#B2B2B2;}
.erbox{text-align:center; width:200px; position:absolute; top:50%; left:50%; margin-left:-100px; margin-top:-53px;}
/*服务网点*/
.switch{position:absolute; line-height:42px; padding:0 1.0rem; top:0;right:0;}
.mtsp{top:46px; position:fixed;width:100%; z-index:999;}
.dis_rel{margin-top:90px;}
.undis_rel{margin-top:90px;}
.pointlist{margin:10px 10px 0px;background:url(../images/fwwd_bg.png); background-size:100% 100%;padding:10px 9px;}
.pointtitle1{font-size:1.6rem;width: 75%;}
.pointtitle2{width: 25%;font-size: 1.4rem;color: #8E8E8E;margin-top:0.3rem;text-align: right;}
.pointline{border-bottom: 1px solid #B2B2B2;padding-bottom:8px;}
.pdetail{line-height: 30px;color: #8E8E8E;}
.address{ position:fixed;left:15px;padding:5px 0px;bottom:0;background:#FFF;width:100%;display:none;}
.address a{display: block;}
.fixed{ position:fixed;right:15px;left:15px;z-index: 10;}
@media screen and (max-width:240px){
	.top{line-height: 22px;}
	.back{padding:0 0.6rem;line-height: 34px;}
	.list{margin:0 0.6rem;}
	.fwjs{margin:2.0rem 0.6rem;padding:15px 6px 10px;}
	.mt{margin-top:38px;}
	.submit{margin:0 6px;}
	.wash{margin: 0.6rem;}
	.subtn{margin-bottom:30px;margin-top: 16px;}
	.az{width:40px;top:38px;}
	.recordlist{margin:10px 6px 0px;}
	.fadetwo1{width: 160px;height: 160px;margin-left:-80px;margin-top:-80px;}
	.fadetwo2{margin: 1rem;}
	.actcontent{padding: 90px 0px 60px}
	.apart{margin: 0px auto 40px;}
	.acttext{line-height:20px;padding: 0px 20px 30px;}
	.switch{padding:0rem 0.6rem;line-height: 34px;}
	.alllist{padding:4px 0px !important;}
	.mtsp{top:38px;}
	.dis_rel{margin-top:75px;}
	.undis_rel{margin-top:75px;}
	.pointtitle1{font-size: 1.4rem;width:70%;}
	.pointtitle2{font-size: 1.2rem;width:30%;}
	.pdetail{font-size: 1.2rem;line-height:2rem;}
	}
@media screen and (min-width:768px){
	.az{line-height:23px;}
	.acttext{padding: 0px 80px 35px;}
	}